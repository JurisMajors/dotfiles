! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #404040, #404040, #000000

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #404040, #d0d0d0, #404040, #404040, #ffffff
rofi.color-active:  #404040, #0066ff, #404040, #404040, #0066ff
rofi.color-urgent:  #404040, #ff0000, #404040, #404040, #ff0000

! Set the desired separator style
rofi.separator-style: solidrofi.color-enabled:true
URxvt.modifier: alt
URxvt.perl-lib: /usr/lib/urxvt/perl/
URxvt.perl-ext-common: default,matcher,url-select,keyboard-select,font-size
! url-jumping
URxvt.keysym.C-u:   perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/firefox
URxvt.url-select.underline: true
! keyboard-slect
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keyboard-select.clipboard: true
! font
URxvt.font:   xft:Fantasque Sans Mono Nerd Font:size=13,xft:Noto Sans:size=11,xft:Terminess Powerline:size12
!URxvt.font: xft:Terminess Powerline:size=12
URxvt.keysym.C-minus:   font-size:decrease
URxvt.keysym.C-equal:    font-size:increase
! transperancy
URxvt.transparent: true
URxvt.shading: 10
! disable that ugliness
URxvt.scrollBar: false
URxvt.letterSpace: 0
URxvt.cursorBlink: 1

URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1
! magenta beauty
URxvt*.cursorColor:  #ff2491
! colorscheme 
URxvt*.foreground:   #d2d2d2
URxvt*.background:   #000000
URxvt*.color0:       #303030
URxvt*.color8:       #5e5e5e
URxvt*.color1:       #c10023
URxvt*.color9:       #ff8781
URxvt*.color2:       #a83334
URxvt*.color10:      #ffa095
URxvt*.color3:       #b51d2c
URxvt*.color11:      #ff948b
URxvt*.color4:       #8c4b46
URxvt*.color12:      #feb2aa
URxvt*.color5:       #7d534f
URxvt*.color13:      #ebbbb5
URxvt*.color6:       #9a413d
URxvt*.color14:      #ffaaa0
URxvt*.color7:       #c6c6c6
